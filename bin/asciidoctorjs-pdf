#!/usr/bin/env node

'use strict'

process.title = 'asciidoctorjs-pdf'
const cli = require('../lib/cli.js')

async function main() {
    const argv = cli.argsParser().argv
    await cli.run(argv)
}

main()
    .then(() => {
        process.exit(0)
    })
    .catch((error) => {
        console.log('error', error)
        process.exit(1)
    })
